#import "<%= klass.name %>+<%= property.name %>.h"
<% if klass.superclass_name %>
<%= "#import \"" + klass.superclass_name + '+' + property.name + ".h\"" %>
<% end %>
#import "DKNightVersionManager.h"

@implementation <%= klass.name %> (<%= property.cap_name %>)

- (UIColor *)defaultNight<%= property.cap_name %> {
    return <% if property.default_color %><%= "UIColorFromRGB(#{property.default_color})" %><% else %><%= 'self.normal' + property.cap_name %><% end %>;
}

@end
